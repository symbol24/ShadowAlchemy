; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ShadowAlchemy"
run/main_scene="res://Scenes/Worlds/test.tscn"
config/custom_user_dir_name="ShadowAlchemy"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

SASignals="*res://Code/Autoloads/signals.gd"
SAInput="*res://Code/Autoloads/player_input.gd"
GameMode="*res://Code/Autoloads/gamemode.gd"
Audio="*res://Code/Autoloads/audio.gd"
UI="*res://Scenes/UI/ui.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="ShadowAlchemy"

[file_customization]

folder_colors={
"res://Audio/": "green",
"res://Code/": "red",
"res://Data/": "purple",
"res://Scenes/": "yellow",
"res://Textures/": "pink"
}

[filesystem]

import/blender/enabled=false

